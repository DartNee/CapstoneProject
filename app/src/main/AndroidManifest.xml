<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.capstoneproject">
    <!-- 인터넷 사용 권한 설정 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".utils.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CapstoneProject"

        android:usesCleartextTraffic="true"
        >
        <activity
            android:name=".Register"
            android:exported="false" />
        <activity
            android:name=".LoginClient"
            android:exported="false" />
        <activity
            android:name=".practice"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/sample" />

        <activity
            android:name=".Manage_Schedule"
            android:exported="false" />
        <activity
            android:name=".Manage_Group"
            android:exported="false" />
        <activity
            android:name=".Participate_Group"
            android:exported="false" />
        <activity
            android:name=".Create_Group"
            android:exported="false" />
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>